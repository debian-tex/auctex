#include "variables"
#!/bin/bash -e
#
# postinst configuration file for Debian auctex package.
# $Id: prerm.in,v 1.2 1999/10/11 08:15:32 salve Exp $
#
<:=@COPYRIGHT:>//

set -o posix

case "$1" in
    remove|upgrade|deconfigure)
	# remove all files generated by <UPDATE>
	test "$1" = "remove" && rm -rf <:=$varauc:>
	# remove emacs startup file
	test "$1" = "remove" -o "$1" = "deconfigure" \
	    && rm -f <:=$sstartd:>/<:=$EPRIORITY:><:=$PACKAGE:>
	install-info --quiet --remove <:=$PACKAGE:>
	<:=$EPREMOVE:> <:=$PACKAGE:>
	test -x <:=$DBINSTALL:> && <:=$DBINSTALL:> --remove <:=$PACKAGE:>
	# Compatibility link.
	test -L /usr/doc/<:=$PACKAGE:> && rm -f /usr/doc/<:=$PACKAGE:>
    ;;
    failed-upgrade)
	:
    ;;
    *)
	echo "${0##*/}: Called with unknown argument \`$1'." >&2
    ;;
esac

exit 0
<:
# Local Variables:
# mode: shell-script
# End:
:>//
